                    <condition>
                        <description><![CDATA[Secteur d'activité]]></description>
                        <expression><![CDATA[$B.secteur_activite === 'BUREAUX']]></expression>
                    </condition>
                    <condition>
                        <description><![CDATA[Zone climatique]]></description>
