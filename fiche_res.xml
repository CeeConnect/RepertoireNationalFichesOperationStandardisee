            </condition>
        </conditions>
        <regles>
            <regle>
                <description><![CDATA[H1]]></description>
                <expression><![CDATA[18300 * $B.appartements]]></expression>
                <conditions>
                    <condition>
                        <description><![CDATA[Zone climatique]]></description>
                        <expression><![CDATA[$O.zone_climatique === 'H1']]></expression>
                    </condition>
                </conditions>
            </regle>
